<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <PackageId>EasyDotNetCliLibrary</PackageId>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageVersion>1.0.1</PackageVersion>
        <Title>A simple .NET CLI Framework</Title>
        <Description>This provides a simple way to use the args[] out of a .NET console application to map to method and classes registered as command handlers.</Description>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Autofac" Version="5.2.0" />
      <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    </ItemGroup>

</Project>
